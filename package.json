{
  "name": "preact-hashish-router",
  "version": "0.0.8",
  "type": "module",
  "module": "dist/index.js",
  "description": "A simple router for preact",
  "scripts": {
    "build": "tsc -p ./tsconfig.lib.json",
    "format": "prettier --write src --ignore-unknown",
    "app:dev": "vite",
    "prepublishOnly": "npm run build && npm run format",
    "push": "npm version patch && git push",
    "push-minor": "npm version minor && git push",
    "push-major": "npm version major && git push"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "peerDependencies": {
    "preact": "^10.26.2"
  },
  "keywords": [
    "preact",
    "router",
    "simple",
    "routing",
    "browser",
    "hash"
  ],
  "target": "es2020",
  "author": "LiasCode",
  "devDependencies": {
    "@preact/preset-vite": "^2.10.1",
    "@types/node": "^22.13.4",
    "preact-hashish-router": "^0.0.6",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ]
}
